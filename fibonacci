a=int(input("Enter the serie number"))
b=0                                         #first element of series
c=1                                         #second element of series
if a<=0:
    print("Invalid series is ",b)
else:
    print(b,c,end="-")
    for x in range(2,a):
        i=b+c                          
        print(i,end="-")
        b=c
        c=i
